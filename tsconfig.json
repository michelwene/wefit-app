{
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "composite": true
  },
  "include": ["src/**/*", "**/*.ts", "**/*.tsx"],
  "extends": "expo/tsconfig.base"
}
